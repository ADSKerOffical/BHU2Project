local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lolpoppyus/Roblox-Lua/master/Pop%20UI%20Lib", true))()

local Tab = library:Tab("Budgie Hub | Cut trees")

Tab:Textbox("Reach tool", function(arg)
   for _, part in next, game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):GetDescendants() do
  if part:IsA("BasePart") then
    part.Size = Vector3.new(tonumber(arg), tonumber(arg), tonumber(arg))
    part.Transparency = 1
    
    local sel = Instance.new("SelectionBox", part)
    sel.Adornee = part
  end
  end
end)
