  Description = "Counter can glitch the one who grab you",
  Default = false,
  Callback = function(Value)
    GlitchCounter = Value
  end
})

Tabs.Defense:AddToggle("Defense", {
  Title = "Enable counter",
  Description = "If someone grabs you, you grab them back",
  Default = false,
  Callback = function(Value)
   euhj = Value
   local con10 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class:IsDescendantOf(game.Players.LocalPlayer.Character) then
    local plr = game.Players:FindFirstChild(tostring(class.Value))
      if plr.Character.Humanoid.SeatPart and plr.Character.Humanoid.SeatPart.Parent.Name == "CreatureBlobman" then return end
    controlPart(plr.Character.HumanoidRootPart)
    if PushCounter == true then
        local bv = Instance.new("BodyVelocity", plr.Character.HumanoidRootPart)
        bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bv.Velocity = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Unit * -1000
      elseif FlingCounter == true then
        local bav = Instance.new("BodyAngularVelocity", plr.Character.HumanoidRootPart)
        bav.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bav.AngularVelocity = Vector3.new(1e9, 1e9, 1e9)
        game.Debris:AddItem(bav, task.wait())
      elseif KillCounter == true then
        plr.Character:BreakJoints()
      elseif GlitchCounter == true then
        plr.Character.HumanoidRootPart.Position += Vector3.new(10000, 10000, 10000)
    end
    letPart(plr.Character.HumanoidRootPart)
  end
end)
repeat task.wait() until euhj == false con10:Disconnect()
  end
})



Tabs.Special:AddButton({
  Title = "Explode black hole bomb in area",
  Description = "It works, but poorly. Double-click quickly to make it work. For your convenience, a tool appears in your inventory",
  Callback = function()
     local tool = Instance.new("Tool", game.Players.LocalPlayer:WaitForChild("Backpack"))
tool.Name = "Spawn bomb"
tool.RequiresHandle = false

game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
tool.Activated:Connect(function()
  local mouse = game.Players.LocalPlayer:GetMouse()
  local pos = mouse.Hit.Position
  
  game.ReplicatedStorage.MenuToys.SpawnToyRemoteFunction:InvokeServer("BombDarkMatter", game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, Vector3.new(0, 0, 0))
  repeat task.wait() until workspace:FindFirstChild(game.Players.LocalPlayer.Name .. "SpawnedInToys"):FindFirstChild("BombDarkMatter") and workspace:FindFirstChild(game.Players.LocalPlayer.Name .. "SpawnedInToys").BombDarkMatter:FindFirstChild("Pyramid")
local bomb = Workspace:FindFirstChild(game.Players.LocalPlayer.Name .. "SpawnedInToys").BombDarkMatter
  repeat task.wait() game.ReplicatedStorage.GrabEvents.SetNetworkOwner:FireServer(bomb.Pyramid, bomb.Pyramid.CFrame) until isnetworkowner(bomb.Pyramid)
    task.wait(wait())
  local bv = Instance.new("BodyVelocity", bomb.Pyramid)
  bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
  bv.Velocity = (mouse.Hit.Position - bomb.Pyramid.Position).Unit * 500
  game:GetService("ReplicatedStorage"):WaitForChild("BombEvents"):WaitForChild("BombExplode"):FireServer({
     Radius = 10, TimeLength = 0.5, DestroysModel = true, ImpactSpeed = 20, ExplodesByFire = true, MaxForcePerStudSquared = -100, Model = bomb, Type = "Implode", ExplodesByPointy = false, Hitbox = bomb:FindFirstChild("PartHitDetector"), PositionPart = bomb:FindFirstChild("PartHitDetector")
    }, pos)
end)
  end
})
