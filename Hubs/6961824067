Tabs.GrabMod:AddToggle("GrabMod", {
  Title = "Massless grab",
  Description = "When you grab a part or humanoid they become very lightweight", 
  Default = false,
  Callback = function(Value)
  ajl = Value
    local con3 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      if class.Parent.Parent:IsA("Model") and class.Parent.Parent:FindFirstChildOfClass("Humanoid") then
      local model = class.Parent.Parent
      for _, part in next, model:GetDescendants() do
        if part:IsA("BasePart") then
         part.CustomPhysicalProperties = PhysicalProperties.new(0, 1, 1, 0, 0)
       end
      end
        else
      local part = class.Parent
        if part.Parent ~= nil then
          for _, ei in next, part.Parent:GetDescendants() do
            if ei:IsA("BasePart") then
              ei.CustomPhysicalProperties = PhysicalProperties.new(0, 1, 1, 0, 0)
            end
          end
        end
      end
    end
  end
end)
repeat task.wait() until ajl == false con3:Disconnect()
  end
})

Tabs.GrabMod:AddToggle("Defense", {
  Title = "Shoot grab",
  Description = "After release, uses the object as a projectile",
  Default = false,
  Callback = function(Value)
   jeijd = Value
   local con5 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
       local part = class.Parent
         repeat task.wait() until not part:FindFirstChild("PartOwner")
       part.CanCollide = false
       part.CustomPhysicalProperties = PhysicalProperties.new(100, 0, 0, 0, 0)
       part.Massless = false
       part.RootPriority = -127
       
       local bav = Instance.new("BodyVelocity", part)
       bav.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
       bav.Velocity = workspace.CurrentCamera.CFrame.LookVector * PGS
       
       local bd = Instance.new("BodyAngularVelocity", part)
       bd.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
       bd.AngularVelocity = Vector3.new(PGS, PGS, PGS)
    end
  end
end)
repeat task.wait() until jeijd == false con5:Disconnect()
  end
})

