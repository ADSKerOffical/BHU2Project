Tabs.GrabMod:AddToggle("Defense", {
  Title = "Flinger grab",
  Description = "When you grab the part he begins to throw away all parts he touches",
  Default = false,
  Callback = function(Value)
 pejnw = Value
local con11 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      local parte = class.Parent
      if parte.Parent ~= workspace then
      for _, part in next, parte.Parent:GetDescendants() do
        if part:IsA("BasePart") then
        task.spawn(function()
        repeat 
          game:GetService("RunService").Heartbeat:Wait()
 local vel = part.Velocity
  part.Velocity = vel * 100000 + Vector3.new(0, 100000, 0)
  game:GetService("RunService").RenderStepped:Wait()
   part.Velocity = vel
  part.Velocity = vel + Vector3.new(0, 0, 0) 
        until not parte:FindFirstChild("PartOwner")
     end)
  end
  end
  end
  end
  end
end)
repeat task.wait() until pejnw == false con11:Disconnect()
  end
})



Tabs.Aura:AddToggle("Pl", {
  Title = "Push Aura",
  Description = "Anyone who comes close will be thrown back",
  Default = false,
  Callback = function(Value)
  yey = Value
while yey and task.wait() do
  for _, part in next, game.Workspace:GetPartBoundsInRadius((game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart").Position, 30) do
    if part.Anchored == false and not part:IsDescendantOf(game.Players.LocalPlayer.Character) then
    local bv = Instance.new("BodyVelocity")
    bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
      if part.Parent ~= nil and part.Parent:IsA("Model") and part.Parent:FindFirstChildOfClass("Humanoid") then
     game.ReplicatedStorage.GrabEvents.SetNetworkOwner:FireServer(part.Parent.HumanoidRootPart, part.Parent.HumanoidRootPart.CFrame)
     bv.Parent = part.Parent.HumanoidRootPart
     bv.Velocity = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - bv.Parent.Position).Unit * -1000
     game.Debris:AddItem(bv, task.wait())
        else
     game.ReplicatedStorage.GrabEvents.SetNetworkOwner:FireServer(part, part.CFrame)
     bv.Parent = part
     bv.Velocity = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - bv.Parent.Position).Unit * -1000
     game.Debris:AddItem(bv, task.wait())
      end
    end
  end
end
  end
})



local Input = Tabs.Blobman:AddInput("Input", {
    Title = "Set blobman speed",
    Description = "Sets the speed of the blobman you are sitting on",
    Default = 20,
    Placeholder = "Set speed",
    Numeric = true, -- Only allows numbers
    Finished = true, -- Only calls callback when you press enter
    Callback = function(Value)
        if game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent.Name == "CreatureBlobman" then
         game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent:FindFirstChildOfClass("Humanoid").WalkSpeed = Value
       end
    end
})



Tabs.Defense:AddToggle("Defense", {
  Title = "Anti grab",
  Description = "Players can\'t grab you",
  Default = false,
  Callback = function(Value)
    asu = Value
local con1 = game.Players.LocalPlayer.IsHeld:GetPropertyChangedSignal("Value"):Connect(function()
  if game.Players.LocalPlayer.IsHeld.Value and not game.Players.LocalPlayer.Character.Humanoid.SeatPart then
    controlPart(game.Players.LocalPlayer.Character.HumanoidRootPart)
    game:GetService("ReplicatedStorage").CharacterEvents.Struggle:FireServer()
    game.Players.LocalPlayer.Character.Humanoid:ChangeState("GettingUp")
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
   local tim = os.clock()

    for _, part in next, game.Players.LocalPlayer.Character:GetDescendants() do
       if part:IsA("BasePart") then
         part.Massless = false
         part.RootPriority = 127
         part:ResetPropertyToDefault("CustomPhysicalProperties")
         part.Anchored = true task.spawn(function() repeat task.wait() until game.Players.LocalPlayer.IsHeld.Value == false part.Anchored = false end)
         part.Velocity = Vector3.zero
         part.AssemblyAngularVelocity = Vector3.zero
       elseif part:IsA("BodyMover") then
         part:Destroy()
      end
    end
   local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    repeat task.wait()
game:GetService("ReplicatedStorage").CharacterEvents.Struggle:FireServer()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos 
until game.Players.LocalPlayer.IsHeld.Value == false and game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored == false
  end
end)
repeat task.wait() until asu == false con1:Disconnect()
  end
})

Tabs.Defense:AddToggle("Defense", {
  Title = "Anti void",
  Description = "You won\'t die if you fall into the void",
  Default = false,
  Callback = function(Value)
     juw = Value
     while juw and task.wait() do
       local human = (game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()):FindFirstChild("HumanoidRootPart")
       if human then
         if (human.CFrame.Y <= workspace.FallenPartsDestroyHeight + 10) or (human.CFrame.Y >= 2000) then
           human.Velocity = Vector3.zero
           human.AssemblyAngularVelocity = Vector3.zero
           human.CFrame = workspace.SpawnLocation.CFrame * CFrame.new(0, 2, 0)
         end
       end
     end
  end
})

Tabs.Defense:AddToggle("Defense", {
  Title = "Anti fire",
  Description = "When you start to burn, you teleport to the fire extinguisher",
  Default = false,
  Callback = function(Value)
     wij = Value
while wij and task.wait() do
  local hum = (game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
  if hum and hum:FindFirstChild("FirePlayerPart") and hum.FirePlayerPart.CanBurn.Value == true then
    local pos = hum.CFrame
    local fireex = function()
      return workspace:FindFirstChild(game.Players.LocalPlayer.Name .. "SpawnedInToys"):FindFirstChild("FireExtinguisher")
    end
    if not fireex() then
      coroutine.wrap(function() game.ReplicatedStorage.MenuToys.SpawnToyRemoteFunction:InvokeServer("FireExtinguisher", hum.CFrame, Vector3.new(0, 0, 0)) end)()
      repeat task.wait() until fireex() ~= nil and fireex():FindFirstChild("SoundPart")
    end
      repeat task.wait() hum.CFrame = fireex().SoundPart.CFrame until hum.FirePlayerPart.CanBurn.Value == false
    hum.CFrame = pos
  end
end
  end
})

