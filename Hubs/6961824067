local Library = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local function GetDevice(): string
 local GS, UIS = game:GetService("GuiService"), game:GetService("UserInputService")
   if (GS:IsTenFootInterface()) then
      return "Console"
  elseif (UIS.TouchEnabled and not UIS.MouseEnabled) then
      return "Mobile"
  else
      return "PC"
  end
end

local function getControlledParts()
  local parts = {}
  for _, part in next, game.Workspace:GetDescendants() do
    if part:IsA("BasePart") and part:FindFirstChild("PartOwner") and part.PartOwner.Value == game.Players.LocalPlayer.Name then
      table.insert(parts, part)
    end
  end
  return parts
end

local function getControlledModels()
  local models = {}
  for _, model in next, game.Workspace:GetDescendants() do
    if model:IsA("Model") and model:FindFirstChildOfClass("Humanoid") and model:FindFirstChild("Head") and model.Head:FindFirstChild("PartOwner") and model.Head.PartOwner.Value == game.Players.LocalPlayer.Name then
      table.insert(models, model)
    end
  end
  return models
end

local function isnetworkowner(part)
  return (part.ReceiveAge == 0 and gethiddenproperty(part, "NetworkIsSleeping") == false)
end

local function isnetworkownerp(part)
  return (part.ReceiveAge == 0 and gethiddenproperty(part, "NetworkIsSleeping") == false) and part:FindFirstChild("PartOwner") and part.PartOwner.Value == game.Players.LocalPlayer.Name
end

local function isnetworkownerm(model)
  return (model.HumanoidRootPart.ReceiveAge == 0 and gethiddenproperty(model.HumanoidRootPart, "NetworkIsSleeping") == false) and model.Head:FindFirstChild("PartOwner") and model.Head.PartOwner.Value == game.Players.LocalPlayer.Name
end

local function controlPart(part: BasePart)
  game.ReplicatedStorage.GrabEvents.SetNetworkOwner:FireServer(part, part.CFrame)
end

local function letPart(part: BasePart)
  game.ReplicatedStorage.GrabEvents.DestroyGrabLine:FireServer(part)
end

local function getBlobMan()
  for _, model in next, game.Workspace:GetDescendants() do
    if model:IsA("Model") and model.Name == "CreatureBlobman" then
      return model
    end
  end 
  return nil
end

local Window = Fluent:CreateWindow({
    Title = "Budgie Hub | FTAP (Alpha)",
    SubTitle = "by ADSKer (ADSKer_man222)",
    TabWidth = 160,
    Size = UDim2.new(0.7, 0, 1, 0),
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

local function GetFrame()
  for _, gui in next, game.CoreGui:GetDescendants() do
    if gui:IsA("TextLabel") and gui.Text:lower():find("budgie hub | ftap") then
      return gui.Parent.Parent.Parent
    end
  end
end

if GetDevice() == "Mobile" then
  game.Players.LocalPlayer.PlayerGui.ScreenOrientation = Enum.ScreenOrientation.LandscapeRight
  
  if GetFrame() then
    local fram = GetFrame()
    local sce = Instance.new("ScreenGui", game.CoreGui)
sce.Name = "ToggleBuguh"
sce.ResetOnSpawn = false
sce.Enabled = true

fram.Parent.Destroying:Connect(function()
  sce:Destroy()
end)

local button = Instance.new("TextButton", sce)
button.Text = "Show window"
button.TextWrapped = true
button.Size = UDim2.new(0.05, 0, 0.1, 0)
button.TextSize = button.TextSize / 1.05
button.Position = UDim2.new(0, 0, 0.3, 0)
button.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
button.BackgroundTransparency = 0.1
button.TextColor3 = Color3.new(1, 1, 1)
button.Active = true
button.Draggable = true

local uicorner = Instance.new("UICorner", button)
uicorner.CornerRadius = UDim.new(0.2, 0)

button.MouseButton1Click:Connect(function()
  fram.Visible = not fram.Visible
end)
  end
end

local Tabs = {
  GrabMod = Window:AddTab({ Title = "Grab mod", Icon = "rbxassetid://17313313998"}),
  Aura = Window:AddTab({ Title = "Auras", Icon = "rbxassetid://129855843582244"}),
  Blobman = Window:AddTab({ Title = "Blob man", Icon = "rbxassetid://81412724248693"}),
  Defense = Window:AddTab({ Title = "Defense", Icon = "rbxassetid://7169354142"}),
  Special = Window:AddTab({ Title = "Special", Icon = "rbxassetid://6523858394"})
}

local GDV, OnlyParts, OnlyHumanoid = 30, false, false
local PushCounter, FlingCounter, GlitchCounter, KillCounter = false, false, false, false
local Input = Tabs.GrabMod:AddInput("Input", {
    Title = "Modify grab distance",
    Description = "Your grab distance increases when you grab something",
    Default = 20,
    Placeholder = "Set",
    Numeric = true,
    Finished = true,
    Callback = function(Value)
        GDV = Value
    end
})

Tabs.GrabMod:AddToggle("Defense", {
  Title = "Filter parts",
  Description = "Modificated grab distance apply only on parts",
  Default = false,
  Callback = function(Value)
    OnlyParts = Value
  end
})

Tabs.GrabMod:AddToggle("Defense", {
  Title = "Filter humanoids",
  Description = "Modificated grab distance apply only on humanoids",
  Default = false,
  Callback = function(Value)
     OnlyHumanoid = Value
  end
})

Tabs.GrabMod:AddToggle("Jie", {
  Title = "Use modify grab distance",
  Description = "You using modified grab distance. I haven\'t been able to increase the distance limit specifically yet",
  Default = false,
  Callback = function(Value)
  jnue = Value
 local con8 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      if class.Parent.Parent:IsA("Model") and class.Parent.Parent:FindFirstChildOfClass("Humanoid") and OnlyHumanoid == true then
        getsenv(game.Players.LocalPlayer.Character.GrabbingScript).distance = GDV or 30
      elseif not class.Parent.Parent: FindFirstChildOfClass("Humanoid") and OnlyParts == true then
        getsenv(game.Players.LocalPlayer.Character.GrabbingScript).distance = GDV or 30
      end
    end
  end
end)
repeat task.wait() until jnue == false con8:Disconnect()
  end
})

local PGS = 1000
local Slider = Tabs.GrabMod:AddInput("Input", {
    Title = "Set push grab strength",
    Description = "",
    Default = 1000,
    Placeholder = "Set",
    Numeric = true,
    Finished = true,
    Callback = function(Value)
        PGS = tonumber(Value)
    end
})

Tabs.GrabMod:AddToggle("GrabMod", {
  Title = "Push grab",
  Description = "When you release the part or humanoid you push it",
  Default = false,
  Callback = function(Value)
     aso = Value
local con2 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      if class.Parent.Parent:IsA("Model") and class.Parent.Parent:FindFirstChildOfClass("Humanoid") then
      local model, part = class.Parent.Parent, class.Parent
        repeat task.wait() until not part:FindFirstChild("PartOwner") if aso == false then return end
      local bv = Instance.new("BodyVelocity", model.HumanoidRootPart)
      bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
      bv.Velocity = workspace.CurrentCamera.CFrame.LookVector * PGS
      game.Debris:AddItem(bv, task.wait())
        else
      local part = class.Parent
        repeat task.wait() until not part:FindFirstChild("PartOwner") if aso == false then return end
      local bv = Instance.new("BodyVelocity", part)
      bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
      bv.Velocity = workspace.CurrentCamera.CFrame.LookVector * PGS
      game.Debris:AddItem(bv, task.wait())
      end
    end
  end
end)
repeat task.wait() until aso == false con2:Disconnect()
  end
})

Tabs.GrabMod:AddToggle("Defense", {
  Title = "Fling grab",
  Description = "When you grab a part or humanoid they disappeared",
  Default = false,
  Callback = function(Value)
  hjw = Value
local con4 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      if class.Parent.Parent:IsA("Model") and class.Parent.Parent:FindFirstChildOfClass("Humanoid") then
       local model = class.Parent.Parent
        local bav = Instance.new("BodyAngularVelocity", model.HumanoidRootPart.RagdollTouchedHitbox)
       bav.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
       bav.AngularVelocity = Vector3.new(1e9, 1e9, 1e9)
       game.Debris:AddItem(bav, task.wait())
        else
       local part = class.Parent
       local bav = Instance.new("BodyAngularVelocity", part)
       bav.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
       bav.AngularVelocity = Vector3.new(1e9, 1e9, 1e9)
       game.Debris:AddItem(bav, task.wait())
      end
    end
  end
end)
repeat task.wait() until hjw == false con4:Disconnect()
  end
})

Tabs.GrabMod:AddToggle("GrabMod", {
  Title = "Kill grab",
  Description = "You kill a player when you grab him",
  Default = false,
  Callback = function(Value)
  euu = Value
local con7 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
      if class.Parent.Parent:IsA("Model") and class.Parent.Parent:FindFirstChildOfClass("Humanoid") then
       local model = class.Parent.Parent
       controlPart(model.HumanoidRootPart)
         model:BreakJoints()
       letPart(model.HumanoidRootPart)
      end
    end
  end
end)
repeat task.wait() until euu == false con7:Disconnect()
  end
})

Tabs.GrabMod:AddToggle("Defense", {
  Title = "Noclip grab",
  Description = "The object you grab will pass through matter",
  Default = false,
  Callback = function(Value)
  kejhe = Value
   local con12 = game.Workspace.DescendantAdded:Connect(function(class)
  if class:IsA("ValueBase") and class.Name == "PartOwner" and class.Value == game.Players.LocalPlayer.Name then
    if class.Parent:IsA("BasePart") and class.Parent.Anchored == false then
